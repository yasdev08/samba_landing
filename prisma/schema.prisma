generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Order {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  phone     String
  wilaya    String
  baladiya  String
  pointure  String
  timestamp DateTime @default(now())
}
