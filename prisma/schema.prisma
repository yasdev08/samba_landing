generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Order {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  product    String
  name       String
  phone      String
  wilaya     String
  baladiya   String
  pointure   String
  createdAt  DateTime @default(now())
}

